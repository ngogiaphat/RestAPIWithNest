{
  "semi": true,
  "tabWidth": 2,
  "useTabs": true,
  "printWidth": 100,
  "endOfLine": "crlf",
  "singleQuote": false,
  "proseWrap": "never",
  "trailingComma": "all",
  "bracketSpacing": false,
  "jsxSingleQuote": false,
  "bracketSameLine": true,
  "quoteProps": "as-needed",
  "jsxBracketSameLine": true,
  "vueIndentScriptAndStyle": true,
  "htmlWhitespaceSensitivity": "css"
}